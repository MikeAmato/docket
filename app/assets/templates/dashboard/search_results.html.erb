<h2>Search Results</h2>
<h4>Click on any event to learn more about it.</h4>

<div>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Title</th>
        <th>Date and Time</th>
      </tr>
    </thead>
    <tbody>
      <tr ng-repeat="event in searchEvents" ng-class="{highlighted: event === selectEvent}" ng-click="setSelectedEvent(event)">
        <td>{{ event.title }}</td>
        <td>{{ event.start_time }}</td>
      </tr>
    </tbody>
  </table>  
</div>


